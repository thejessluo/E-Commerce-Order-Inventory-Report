	partition Product_Daily_Sales = calculated
		mode: import
		source = ```
				--calculates sum of qty sold by productID by day across all retailers. Excludes WFS
				SELECTCOLUMNS(
				    GROUPBY(
				        ADDCOLUMNS(
				            FILTER(
				                bvc_OrderItem,
				                bvc_OrderItem[OrderSource - CompanyName] <> "WFS - Tzumi Electronics"
				            ),
				            "SaleDate", TRUNC(bvc_OrderItem[RecordCreatedOn])
				        ),
				        bvc_OrderItem[ProductID],
				        [SaleDate],
				        "DailyQty", SUMX(CURRENTGROUP(), bvc_OrderItem[Qty])
				    ),
				    "ProductID", bvc_OrderItem[ProductID],
				    "SaleDate", [SaleDate],
				    "DailyQty", [DailyQty]
				)
				
				```

	annotation PBI_Id = 7162ed0d0dae489ebfb5db08347ed981

