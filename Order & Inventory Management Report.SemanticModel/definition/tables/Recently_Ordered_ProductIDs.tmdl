	partition Recently_Ordered_ProductIDs = calculated
		mode: import
		source = ```
				--table used to calculate zero-stock SKUs that have not had an order in the last 4 weeks
				DISTINCT(
				    SELECTCOLUMNS(
				        FILTER(
				            'bvc_OrderItem',
				            'bvc_OrderItem'[RecordCreatedOn] >= TODAY() - 28
				        ),
				        "ProductID", 'bvc_OrderItem'[ProductID]
				    )
				)
				
				```

