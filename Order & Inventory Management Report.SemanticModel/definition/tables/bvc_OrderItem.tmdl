	partition bvc_OrderItem = m
		mode: import
		source = ```
				let
				    Source = Sql.Database("10.13.0.206", "SC", 
				        [Query="
				            WITH FilteredOrders AS (
				                SELECT ID
				                FROM bvc_Order WITH (NOLOCK)
				                WHERE RecordCreatedOn >= DATEADD(MONTH, -12, GETDATE())
				            )
				            SELECT oi.*
				            FROM bvc_OrderItem oi WITH (NOLOCK)
				            INNER JOIN FilteredOrders o ON oi.OrderID = o.ID;
				        "]
				    )
				in
				    Source
