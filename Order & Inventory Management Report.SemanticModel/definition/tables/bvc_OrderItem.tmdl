table bvc_OrderItem
	lineageTag: edc2320c-5534-46a9-945e-4470b14b468e

	measure COUNT_Shipped_Orders_BVC_ORDERITEM = ```
			
			CALCULATE(
			    DISTINCTCOUNT(bvc_OrderItem[OrderID]),
			    BVC_ORDERITEM[shippingstatus_mapping] = "FullyShipped"
			)
			
			```
		formatString: 0
		displayFolder: 1Measures/Columns
		lineageTag: fd721a74-3f40-4b2f-ae29-031efa996c79

	measure LatestTimeOfOrder_Item = ```
			MAX('bvc_OrderItem'[TimeOfOrder])
			
			```
		formatString: Short Date
		displayFolder: 1Measures/Columns
		lineageTag: c886b197-fc2f-45e5-b9ee-097aee86f5f8

	measure Item_Weight_Latest = ```
			
			VAR LatestDate =
			    CALCULATE(
			        MAX('bvc_OrderItem'[ShipDate])
			    )
			RETURN
			    CALCULATE(
			        MAX(bvc_OrderItem[Weight]),
			        'bvc_OrderItem'[ShipDate] = LatestDate
			    )
			
			```
		displayFolder: 1Measures/Columns
		lineageTag: ed265557-d362-4a48-92dd-7447b77f3d86

		annotation PBI_FormatHint = {"isGeneralNumber":true}

