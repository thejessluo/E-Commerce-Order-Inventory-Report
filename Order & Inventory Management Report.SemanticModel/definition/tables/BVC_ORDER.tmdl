	column Created_Before_2_30PM_Yesterday_Adjusted = ```
			
			VAR TodayDate = TODAY()
			VAR TodayWeekday = WEEKDAY(TodayDate, 2)  -- 1 = Monday, 7 = Sunday (ISO)
			VAR CutoffDateTime =
			    SWITCH(
			        TRUE(),
			        TodayWeekday = 1,  // Monday
			            TodayDate - 3 + TIME(14, 30, 0),  // Friday 2:30 PM
			        TRUE,
			            TodayDate - 1 + TIME(14, 30, 0)   // Normal case: Yesterday 2:30 PM
			    )
			RETURN
			IF(
			    'bvc_Order'[RecordCreatedOn] < CutoffDateTime,
			    TRUE,
			    FALSE
			)
			
			```
		formatString: """TRUE"";""TRUE"";""FALSE"""
		displayFolder: 1Columns
		lineageTag: 680aa6c4-a69f-47a3-a4ba-f09f50f7c4b0
		summarizeBy: none

